page{ background:#eee !important;padding-bottom:120rpx;} 
.listbox{padding-left:30rpx;background:#fff;}
.szlist{overflow:hidden;zoom:1; background:#fff;height:100rpx;line-height:100rpx;border-bottom:2rpx solid #eee;position:relative;}  
.szlist text{float:left;color:#999; font-size:32rpx;display:block;width:140rpx;}
.szlist input{float:left;color:#333;font-size:32rpx;margin-top:28rpx;margin-left:15rpx;width:78%; }
.daijian::after{ content:'';  width:15rpx;height:23rpx;right:20rpx;top:35rpx; position:absolute;  background-position-x:right;   background:url("data:image/png;base64,") no-repeat; background-size:100% 100%; }
.shebot{padding:20rpx 30rpx; overflow:hidden;zoom:1;}
.shebot .titletext{color:#999; font-size:32rpx; display:block;margin-bottom:20rpx;}
map{margin-bottom:20rpx;}
.huoqubox{position:relative;overflow:hidden;zoom:1;float:left;margin-left:20rpx;}
.huoqubox button{width:300rpx;height:300rpx;float:left;color:#999;line-height:300rpx;font-size:80rpx; position:relative;z-index:100;}
.huoqubox text{float:left; width:340rpx;color:#999;font-size:28rpx;margin-left:50rpx;margin-top:20rpx;}
.huoqubox image{ position:absolute;left:0;z-index:200;}
.botbox{position:fixed;bottom:0;width:100%;height:100rpx;line-height:100rpx;display:flex;  z-index: 300;}
.botbox button{display:block;width:50%;font-size:32rpx; text-align:center;}
.botbox button.quxiao{background:#fff;color:#333;}
.botbox button.seven{background:#ffac40;color:#fff;}


.ordertitle{text-align:center;   font-size:26rpx;color:333; margin-top:35rpx;margin-bottom:15rpx;}
.ordertimebox{display:flex; background:#fff;padding:40rpx 0;border-bottom:1px solid #d1d1d1; position:relative;}
.ordertimtim{ position:absolute; left:42%; background:#fff; z-index:100; font-size:24rpx;color:#999; border:2rpx solid #d1d1d1; border-radius:15rpx;width:80rpx;padding:6rpx 15rpx; display:block; text-align:center;  } 
.odtmcent{width:50%; text-align:center;}
.odtimeleft{position:relative;}
.odtimeleft::after{position:absolute;content:"";display:block;width:2rpx;height:110%;right:0;top:-10rpx;background:#d1d1d1}
.odtmcent image{width:42rpx;height:36rpx;margin-bottom:10rpx;}
.odtmcent text{display:block; font-size:28rpx; color:#333;}