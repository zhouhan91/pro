page{background:#eee;padding-bottom:95rpx;}
.adsbox{ background:#fff url("data:image/gif;base64,");height:128rpx;width:180rpx;overflow:hidden;zoom:1;width:100%; background-repeat:no-repeat; background-position:bottom; background-size:100%; padding-top:52rpx;}
.adsbox .left{float:left;width:40rpx;height:46rpx;margin-left:30rpx;margin-top:20rpx;}
.adsbox .left image{width:100%;height:100%;}
.adsbox .right{float:left;margin-left:28rpx;}
.adsbox .right .name,.adsbox .right .phone{font-size:32rpx;color:#333;padding-right:20rpx;}
.adsbox .right .adss{font-size:26rpx;color:#999; display:block;margin-top:0rpx;}
.ddbox{ background:#fff; border:2rpx solid #d1d1d1; border-radius:10rpx;width:89%;padding:0 3%; margin-left:2.5%;margin-top:15rpx;padding-bottom:30rpx;}
.foodname{margin-top:25rpx;padding-bottom:15rpx;}
.foodname .name{ display:block;font-size:32rpx;color:#333;text-align:center;}
.foodname .bianhao{ display:block; font-size:20rpx;color:#999; text-align:center;}
.footbox{margin:15rpx 0;border-top:2rpx solid #eee;border-bottom:2rpx solid #eee;padding:15rpx 0;}
.footbox .footlist{overflow:hidden;zoom:1;height:60rpx;line-height:60rpx;}
.footlist .left{float:left;width:50%; font-size:26rpx;color:#333;}
.footlist .center{float:left;width:25%; font-size:26rpx;color:#333;}
.footlist .right{float:right;width:25; font-size:26rpx;color:#333;}
.peisong{padding-bottom:30rpx;border-bottom:2rpx solid #eee; overflow:hidden;zoom:1;}
.tebox{overflow:hidden;zoom:1;margin-bottom:20rpx;}
.tebox .tetype{ background:#ff676e;color:#fff;font-size:20rpx; display:block;float:left;width:30rpx;height:30rpx; text-align:center;line-height:30rpx; border-radius:3rpx;margin-right:20rpx;}
.tebox .tetext{display:block;float:left;color:#333;font-size:20rpx;}
.yhlist{overflow:hidden;zoom:1;}
.yhlist .left{float:left;margin-top:15rpx;}
.yhlist .right{float:right;margin-top:10rpx;}
.yhlist .right text{color:#333;font-size:26rpx}
.kouweibox .title{display:block;font-size:26rpx;color:#333; margin-bottom:20rpx;}
.kouwei{ display:flex; flex-wrap:wrap;}
.kouwei text{ display:block;border:2rpx solid #eee;padding:12rpx 18rpx;border-radius:10rpx;color:#999;font-size:26rpx;margin-right:10rpx; margin-bottom:15rpx;}
.beizhu input{border:2rpx solid #eee;height:80rpx;line-height:80rpx; color:#333;font-size:26rpx;padding-left:20rpx;margin-top:15rpx;}
.botbox{height:100rpx;line-height:100rpx; display:flex; position:fixed;bottom:0;width:100%;background:#fff; z-index:10; border-top:2rpx solid #eee;}
.botbox button{display:block;width:50%; text-align:center;font-size:32rpx;color:#333;}
.botbox button.right{ background:#ffac40;color:#fff;}
.botbox button.left::after{display:none;}
.botbox button.left::before{display:none;}